{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "DELETE"
      ]
    }
  ],
  "env": {
    "DATABASE_URL": "postgres://postgres:postrges@localhost/reviews_kg"
  }
}
